package com.extendTest;

public class Animal {
	protected int age;
	protected String name;
	
	public Animal() {
		this.age = 0;
		this.name = "unkonw";
	}
	public Animal(int iAge, String strName) {
		this.age = iAge;
		this.name = strName;
	}
	
	public void eat() {
		System.out.println("eating");
	}
	public void speak() {
		System.out.println("speaking");
	}
}


