package com.extendTest;

public class Dog extends Animal{
		private String color;
		
		public Dog(int age, String name, String color) {
			super(age, name);
			this.color = color;
		}
		public void eat() {
			System.out.println("meat");
		}
		public void speak() {
			System.out.println("wang");
		}
		
	}


